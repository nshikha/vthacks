<script src="/socket.io/socket.io.js"></script>
<script src="/javascripts/jquery-v2.1.1-beta1.js"></script>
<script src="/javascripts/keyboard.js"></script>
<script>
  var socket = io.connect('http://localhost:3000');
  socket.on('init', function (data) {
    if (data === 'snake') {
        $('body').append('<script src="/javascripts/Snake.js"><'+'/script>');
    } else {
        $('body').append('<script src="/javascripts/Controller.js"><'+'/script>');
    }
    start(socket);
  });
</script>
